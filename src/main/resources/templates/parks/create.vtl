<p class="pageHeader">New Park</p>

<form action="/parks" method="post">
    <label for="name">Name:</label>
    <input type="text" class="inputBox" name="name" required="true" id="name">

    <label for="till">Wallet:</label>
    <input type="number" class="inputBox" name="till" required="true" id="till">

    <label for="capacity">Age:</label>
    <input type="number" class="inputBox" name="capacity" required="true" id="capacity">

    <input type="submit" value="Save" class="button">
</form>